<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<html>
<head>
    <title>Convert USD from VND</title>
</head>
<body>
<h1>Convert USD form VND</h1>
<table>
    <tr>
        <td>Rate</td>
        <td><input type="text" value="${rate}"></td>
    </tr>
    <tr>
        <td>USD</td>
        <td><input type="text" value="${usd}"></td>
    </tr>
    <tr>
        <td>VND</td>
        <td><input type="text" value="${vnd}"></td>
    </tr>
</table>
</body>
</html>
